{
  "name": "starter-package-ts",
  "version": "1.10.0",
  "description": "starter-package-ts description",
  "author": "Romain Prignon <pro.rprignon@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romainPrignon/starter-package-ts.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "12",
    "npm": "6"
  },
  "files": [
    "dist",
    "type",
    "LICENSE.md",
    "package-lock.json",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "postinstall": "gitmoji --init",
    "compile": "rm -rf dist/ && tsc -p src/",
    "lint": "npm run lint:src && npm run lint:spec && npm run lint:test",
    "lint:src": "tslint --project src/tsconfig.json",
    "lint:spec": "tslint --project spec/tsconfig.json",
    "lint:test": "tslint --project test/tsconfig.json",
    "test": "npm run test:unit && npm run test:spec",
    "test:one": "jest --detectOpenHandles --forceExit",
    "test:coverage": "npm run test:unit:coverage && npm run test:spec:coverage",
    "test:unit": "jest --detectOpenHandles --forceExit test/",
    "test:unit:coverage": "npm run test:unit -- --coverage",
    "test:unit:watch": "npm run test:unit -- --watch",
    "test:spec": "jest --detectOpenHandles --forceExit spec/",
    "test:spec:coverage": "npm run test:spec -- --coverage",
    "test:spec:watch": "npm run test:spec -- --watch",
    "changelog": "gitmoji-changelog",
    "version": "npm run changelog && git add CHANGELOG.md",
    "version:patch": "npm version --no-commit-hooks patch -m 'ðŸ”– Release v%s'",
    "version:minor": "npm version --no-commit-hooks minor -m 'ðŸ”– Release v%s'",
    "version:major": "npm version --no-commit-hooks major -m 'ðŸ”– Release v%s'",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "pinst --disable && npm run compile",
    "postpublish": "pinst --enable"
  },
  "dependencies": {
    "@sindresorhus/is": "2",
    "highland": "2",
    "immer": "5",
    "make-error-cause": "2",
    "moment": "2",
    "runtypes": "4"
  },
  "devDependencies": {
    "@types/jest": "24",
    "@types/highland": "2",
    "@types/node": "12",
    "gitmoji-changelog": "2",
    "gitmoji-cli": "3",
    "husky": "3",
    "jest": "24",
    "pinst": "2",
    "ts-jest": "24",
    "tslint": "5",
    "tslint-config-standard": "9",
    "tslint-immutable": "6",
    "typescript": "3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run compile && npm run test:coverage"
    }
  }
}
