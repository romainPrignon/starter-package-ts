{
  "name": "starter-package-ts",
  "version": "1.0.0",
  "description": "module description",
  "author": "Romain Prignon <pro.rprignon@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/romainPrignon/starter-package-ts.git"
  },
  "main": "main.js",
  "engines": {
    "node": ">=4.x",
    "npm": ">= 2.x"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clean": "npm cache clean && rimraf -- node_modules typings coverage dist",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commitmsg": "conventional-changelog-lint -e",
    "coverage": "istanbul cover npm run test",
    "lint": "tslint 'src/**/*.ts' 'spec/**/*.ts'",
    "postinstall": "npm run typings install",
    "precommit": "npm run lint && npm run test",
    "test": "ts-node ./node_modules/.bin/jasmine",
    "typings": "typings"
  },
  "dependencies": {
    "config": "latest",
    "core.js": "latest",
    "immutable": "latest",
    "rxjs": "latest"
  },
  "devDependencies": {
    "conventional-changelog-cli": "latest",
    "conventional-changelog-lint": "latest",
    "husky": "latest",
    "istanbul": "latest",
    "jasmine": "latest",
    "remap-istanbul": "latest",
    "rimraf": "latest",
    "ts-helpers": "latest",
    "ts-node": "latest",
    "tslint": "latest",
    "typescript": "latest",
    "typings": "latest"
  }
}
